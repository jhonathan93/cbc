{
    "name": "cbc/cbc",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "authors": [
        {
            "name": "Jhonathan da Silva"
        }
    ],
    "require": {
        "php": "^7.3",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
